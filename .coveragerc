[run]
source = services
omit = 
    */tests/*
    */__pycache__/*
    */venv/*
    */.venv/*
    */site-packages/*
    */htmlcov/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

precision = 2
show_missing = True

[html]
directory = htmlcov

[xml]
output = coverage.xml
